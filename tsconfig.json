{
	"compilerOptions": {
		"outDir": "dist",
		"sourceMap": true,
		"target": "ESNext",
		"module": "CommonJS",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"noImplicitAny": true,
		"strict": true,
		"skipDefaultLibCheck": true
	},
	"include": ["./*.js", "./src/**/*.ts", "./api/*.ts", "./test/**/*.ts"]
}
